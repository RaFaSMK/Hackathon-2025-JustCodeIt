generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Procedimento {
  id                   String    @id @default(uuid())
  codigo               String?
  terminologia_eventos String
  correlacao           String?
  procedimento         String?
  resolucao_normativa  String?
  vigencia             DateTime?
  od                   String?
  amb                  String?
  hco                  String?
  hso                  String?
  pac                  String?
  dut                  String?
  subgrupo             String?
  grupo                String?
  capitulo             String?
  tipo_assinatura      String
}

model Consulta {
  id            Int      @id @default(autoincrement())
  cidade        String
  especialidade String
  medico        String?
  dataHora      DateTime
  protocolo     String   @unique
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
